{
  "name": "nambiquara",
  "version": "1.0.0",
  "description": "A js-to-excel (xml/xlsx) tool.",
  "scripts": {
    "build:clear": "rimraf lib dist",
    "build:typescript": "tsc",
    "build:rollup": "rollup -c",
    "build": "build:clear && npm run build:typescript && npm run build:rollup",
    "test": "jest",
    "tdd": "jest --watch",
    "dev": "npm run build:clear && tsc --pretty --watch",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "docs": "typedoc src"
  },
  "author": "Iguatemi Garin <iguatemi@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1"
  }
}
